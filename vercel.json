{
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/index", "dest": "/index.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/signup", "dest": "/signup.html" },
    { "src": "/marketplace", "dest": "/marketplace.html" },
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/(.+)", "dest": "/$1.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}